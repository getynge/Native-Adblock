<Window x:Class="NativeAdblock.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NativeAdblock"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        Title="Native Adblock" Height="296" Width="515.333">
    <Grid Margin="0,0,25,0">
        <ListBox HorizontalAlignment="Left" Height="100" Margin="44,56,0,0" VerticalAlignment="Top" Width="412" Cursor="None">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <TextBox x:Name="sourceURL"></TextBox>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
        <Button Content="+" HorizontalAlignment="Left" Margin="428,166,0,0" VerticalAlignment="Top" Width="28"/>
        <Button Content="-" HorizontalAlignment="Left" Margin="395,166,0,0" VerticalAlignment="Top" Width="28" IsEnabled="False"/>
        <Button Content="Enable" HorizontalAlignment="Left" Margin="381,223,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
